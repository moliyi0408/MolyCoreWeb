@model IEnumerable<MolyCoreWeb.Models.DBEntitiy.User>
@*  @model IEnumerable<MolyCoreWeb.Models.DTOs.UserDto> *@

<h1>Index</h1>

 <table class="table">
    <thead>
            <tr>
                <th>User ID</th>
                <th>Username</th>
                <th>Password</th>
            </tr>      
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
                <td>@user.UserId</td>
                <td>@user.UserName</td>
                <td>@user.PasswordHash</td>                   
        }
    </tbody>
</table>
